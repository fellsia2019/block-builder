{
  "name": "block-builder",
  "version": "1.0.0",
  "description": "Блочный конструктор для no-code разработки",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.cjs",
    "dev:build": "rollup -c rollup.dev.cjs -w",
    "dev:server": "node dev-server.js",
    "dev": "concurrently -k -n BUILD,SERVER -c blue,green \"npm:dev:build\" \"npm:dev:server\"",
    "devф": "npm run dev",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "start": "node dev-server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "block-builder",
    "no-code",
    "constructor",
    "vue3",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "concurrently": "^9.0.1",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}
